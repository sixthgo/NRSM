<div ng-controller="StorePopupCtrl">
    <!--<h3>현재선택한 지점</h3>-->
    <div class="list">
        <ion-radio ng-repeat="item in data.stores"
                    ng-value="item.plantName"
                    ng-change="OnChange(item)"
                    ng-model="popup.selectedStoreTemp.plantCode">
            {{ item.plantName }}
        </ion-radio>
    </div>
</div>
