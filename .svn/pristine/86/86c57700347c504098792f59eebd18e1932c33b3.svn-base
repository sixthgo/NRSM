'use strict';

angular.module('starter.services')

.factory('POGService', function ($resource, webServerUrl) {
    return $resource(webServerUrl + '/api/POG', {
        plantCode: '@plantCode',
        firstCategory: '@firstCategory',
        thirdCategory: '@thirdCategory',
        column: '@column',
        userName: '@userName',
        goodsCode: '@goodsCode'
    },
    {
        'query': { method: 'GET', isArray: true },
        'get': { method: 'GET' }
    });
})

