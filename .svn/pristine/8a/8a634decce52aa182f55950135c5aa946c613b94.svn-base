<div ng-controller="StorePopupCtrl">
    <div class="list">
        <ion-radio ng-repeat="item in data.stores"
                    ng-value="item"
                    ng-change="OnChange(item)"
                    ng-model="storePopup.selectedStoreTemp">
            {{ item.plantName }}
        </ion-radio>
    </div>
</div>
