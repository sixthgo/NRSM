<ion-view view-title="Common">

    <ion-nav-bar class="{{setting.header.class}}">
        <ion-nav-back-button class="button-clear">
            <i class="icon ion-ios-arrow-left"></i>BACK.
        </ion-nav-back-button>
        <ion-nav-buttons side="right">
            <button class="button button-icon ion-refresh" ng-click="tempReload()"></button>
            <button class="button button-icon ion-calendar" ng-if="base.viewId == 'NRSM011'"></button>
        </ion-nav-buttons>
    </ion-nav-bar>

    <ion-nav-view name="subContent" animation="slide-left-right"></ion-nav-view>

    <footer></footer>
</ion-view>
