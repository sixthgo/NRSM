.item-eland-width {
    width: 100%;
}

.eland-badge {
    display: -webkit-box;
    display: -webkit-flex;
    display: -moz-box;
    display: -moz-flex;
    display: -ms-flexbox;
    display: flex;
    position: absolute;
    top: 16px;
    right: 32px;
    color: black;
    font-size: 15px;
}

.eland-item {
    font-size: 15px;
    color: black;
    letter-spacing: -0.5px;
    border-right: none;
    border-left: none;
}

.list {
    width: 95%;
    margin: 0 auto;
}

.list .eland-item {
    padding-top: 14px;
    padding-left: 2.5%;
    padding-right: 2.5%;
}

.sub-list .list{
    width: 90%;
    margin: 0 auto;
}

.sub-list {
    width: 47%;
    float: left;
    border-top: none;
    border-right: none;
}

.sub-list .list .eland-item {
    padding-top: 14px;
    padding-left: 5%;
    padding-right: 5%;
}

.eland-font-color-green {
    color: #158f54;
}

.row a {
    display: block;
    border: 2px solid #ccc;
    color: #ccc;
    font-weight: 600;
    line-height: 36px;
    text-align: center;
    text-decoration: none;
    letter-spacing: 0.5px;
}

.row a:hover {
    opacity: 0.5;
}