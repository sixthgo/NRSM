'use strict';

angular.module('starter.services')

.factory('CUService', function ($resource, webServerUrl) {
    return $resource(webServerUrl + '/api/cu?loginId=:loginId&type=:type&cuCode=:cuCode', {
        loginId: '@loginId',
        type: '@type',
        cuCode: '@cuCode'
    },
    {
        'query': { method: 'GET', isArray: true }
    });
})

