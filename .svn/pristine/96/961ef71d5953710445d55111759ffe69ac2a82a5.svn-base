/*
footer css
*/

#footer { width:100%; border-top:1px solid #ddd; position:fixed; left:0; bottom:0; background:#fff; }
#footer ul { overflow:hidden; width: 100%;}
#footer ul li { width:20%; height:30px; float:left; }
#footer ul li a { display:block; width:100%; height:100%; text-align:center; }
#footer ul li a img { width:auto; height:100%; }

.select {
    width: 100%;
}

.select .item {
    padding: 0;
}

.select .row.list {
    height: 44px;
    bottom: 10px;
}

.select .row .col, .eland-table-header div.row, .eland-table-body div.row {
    text-align: center;
}

.eland-table-header {
    padding: 0;
    border-left: none;
    border-right: none;
    border-top: none;
}

.eland-table-header .row.list {
    padding: 0;
}

.eland-table-header div.row a {
    display: inherit;
}

.eland-table-body {
    padding: 0;
    border-left: none;
    border-right: none;
}

.eland-col-offset-7 {
    margin-left: 7%;
}

.col-offset-2 {
    margin-left: 3%;
}

.eland-col-6 {
    -webkit-box-flex: 0;
    -webkit-flex: 0 0 19%;
    -moz-box-flex: 0;
    -moz-flex: 0 0 19%;
    -ms-flex: 0 0 19%;
    flex: 0 0 19%;
    max-width: 19%; }

.eland-radio-list .item {
    border-right: none;
    border-left: none;

}

.eland-radio-list .eland-item-fixed {
    padding: 14px;
}

.eland-radio-list .item div {
    padding-left: 14px;
    padding-bottom: 14px;
    padding-top: 14px;
}

.eland-radio-list .item-radio .radio-icon {
    padding: 8px;
}

.eland-radio-list .item-radio input {
    /* hide any radio button inputs elements (the ugly circles) */
    position: absolute;
    left: -9999px; }
.eland-radio-list .item-radio input:checked ~ .item-content {
    /* style the item content when its checked */
    background: #f7f7f7; }
.eland-radio-list .item-radio input:checked ~ .radio-icon {
    /* show the checkmark icon when its checked */
    visibility: visible; }

