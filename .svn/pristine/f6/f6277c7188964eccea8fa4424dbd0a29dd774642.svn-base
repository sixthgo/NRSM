    $scope.ClickStar = function () {
        $scope.debug.Log('ManagerReportCtrl.ClickStar()');
    }

    $scope.ClickGear = function () {
        $scope.debug.Log('ManagerReportCtrl.ClickGear()');
    }

    $scope.ClickAdd = function () {
        $scope.debug.Log('ManagerReportCtrl.ClickAdd()');
    }

    $scope.Init();

});
