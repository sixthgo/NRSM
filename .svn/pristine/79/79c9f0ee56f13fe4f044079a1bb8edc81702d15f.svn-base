'use strict';

angular.module('starter.services')

.factory('CEOReportService', function ($resource) {
    return $resource('/api/CEOReport?gubun=:gubun&group=:group', {
            gubun: '@gubun',
            group:'@group'
    },
    {
        'query': { method: 'GET', isArray: true },
        'save': {method: 'POST'}
    });
})

