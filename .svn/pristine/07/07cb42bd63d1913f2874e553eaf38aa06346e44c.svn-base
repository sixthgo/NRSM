'use strict';

angular.module('starter.services')

.factory('FavoriteService', function ($resource, webServerUrl) {
    return $resource( webServerUrl + '/api/favorite?loginId=:loginId', {
        loginId: '@loginId'
    },
    {
        'query': { method: 'GET', isArray: true },
        'save': { method: 'POST' },
        'put': { method: 'put' }
    });
})

