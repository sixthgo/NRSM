                                        <td class="width13P">{{item.amount | number}}</td>
                                        <td class="width13P">{{item.rate}}%</td>
                                        <td class="width13P">{{item.pastAmount | number}}</td>
                                        <td class="width13P">{{item.growthRate}}%</td>
                                    </tr>
                                </table>
                            </ion-scroll>
