<div ng-controller="CuPopupCtrl">
    <!--<h3>현재선택한 지점</h3>-->
    <ion-content onscroll="false">
        <div class="row">
            <div class="col col50">
                <ion-scroll direction="y" class="popup-scroll">
                    <!--<ul class="list">
                        <li class="item">AAAAAAAAAAAa</li>
                        <li class="item">AAAAAAAAAAAa</li>
                        <li class="item">AAAAAAAAAAAa</li>
                        <li class="item">AAAAAAAAAAAa</li>
