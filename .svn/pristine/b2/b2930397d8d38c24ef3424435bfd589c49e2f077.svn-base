'use strict';

angular.module('starter.services')

.factory('PlantService', function ($resource, webServerUrl) {
    return $resource(webServerUrl + '/api/plant?loginId=:loginId', {
        loginId: '@loginId'
    },
    {
        'query': { method: 'GET', isArray: true }
        , 'get': { method: 'GET' }
    });
})

